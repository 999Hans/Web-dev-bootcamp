<%- include("partials/header") -%>
<h1>Compose</h1>
<form method="post">
  <h4>Title</h4>
  <input
    type="text"
    class="form-control"
    aria-describedby="basic-addon1"
    name="addTitle"
  />

  <h4>Post</h4>
  <div class="form-group">
    <textarea class="form-control" name="addPost" rows="5"></textarea>
  </div>
  <button
    type="submit"
    class="btn btn-primary post-btn"
    aria-label="With textarea"
  >
    Publish
  </button>
</form>
<%- include("partials/footer") -%>
